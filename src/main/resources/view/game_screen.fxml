<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.kpfu.itis.lobanov.controller.GameScreenController"
            fx:id="gameScreen">

    <Pane fx:id="gameWindow" AnchorPane.topAnchor="20">

    </Pane>

    <HBox fx:id="gameInfo" AnchorPane.leftAnchor="5" AnchorPane.rightAnchor="5">
        <Label fx:id="scoreInfo" prefWidth="130"/>
        <Label fx:id="timeLabel" prefWidth="150"/>
    </HBox>

</AnchorPane>
