<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.kpfu.itis.lobanov.controller.CreateRoomScreenController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <VBox AnchorPane.topAnchor="15.0" AnchorPane.leftAnchor="20">
            <Button fx:id="backBtn" text="%go_back"/>
        </VBox>
        <VBox fx:id="panel" alignment="CENTER" spacing="10" AnchorPane.topAnchor="35" AnchorPane.rightAnchor="10"
              AnchorPane.leftAnchor="10">
            <Label text="%input.port" styleClass="label-small"/>
            <TextField fx:id="setPort"/>
            <Button fx:id="changeBtn" text="%map.change"/>
        </VBox>
        <VBox AnchorPane.bottomAnchor="40" AnchorPane.rightAnchor="20" AnchorPane.leftAnchor="20" alignment="CENTER">
            <Button fx:id="submit" text="%server.start"/>
        </VBox>
    </children>

</AnchorPane>
