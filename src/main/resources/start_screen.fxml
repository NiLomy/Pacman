<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.web.WebView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.kpfu.itis.lobanov.controller.StartScreenController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <VBox fx:id="imageBox" AnchorPane.rightAnchor="20" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="20"
              alignment="CENTER" spacing="30">
            <Label text="Pacman Game" style="-fx-arc-height: 20;"/>
        </VBox>

        <VBox alignment="CENTER" spacing="10" AnchorPane.topAnchor="290" AnchorPane.rightAnchor="10"
              AnchorPane.leftAnchor="10">
            <Button fx:id="startPlay" prefWidth="0" text="%start.play" VBox.vgrow="ALWAYS" minWidth="100.0"/>
            <Button fx:id="settings" prefWidth="0" text="%settings" VBox.vgrow="ALWAYS" minWidth="100.0"/>
        </VBox>
    </children>

</AnchorPane>
